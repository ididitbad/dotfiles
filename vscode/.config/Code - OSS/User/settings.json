{
    "window.restoreWindows": "all",
    "window.titleBarStyle": "custom",
    "window.zoomLevel": 0,
    "window.menuBarVisibility": "default",
    "editor.formatOnPaste": true,
    "editor.formatOnType": true,
    "editor.minimap.enabled": true,
    "editor.renderWhitespace": "boundary",
    // "editor.rulers": [ 80, 120 ],
    "search.showLineNumbers": true,
    "search.smartCase": true,
    "files.autoSave": "onFocusChange",
    "debug.inlineValues": true,
    "workbench.editor.highlightModifiedTabs": true,
    "workbench.editor.tabCloseButton": "off",
    "workbench.settings.openDefaultKeybindings": true,
    "workbench.settings.openDefaultSettings": true,
    "git.enableSmartCommit": true,
    "git.autofetch": true,
    "git.confirmSync": false,
    // "python.pythonPath": "/home/ididitbad/py3env/bin/python3",
    // "C_Cpp.default.cStandard": "c99",
    // "C_Cpp.default.cppStandard": "c++14",
    // "C_Cpp.default.includePath": [
    //     "/usr/local", "/usr/local/include",
    //     "/usr/include", "/usr/include/linux", "/usr/include/c++", 
    //     "/usr/lib/gcc/x86_64-pc-linux-gnu/9.1.0/include"
    // ],
    // "gitlens.hovers.currentLine.over": "line",
    "explorer.confirmDelete": false,
    "git.allowForcePush": true,
    "git.showPushSuccessNotification": true,
    "git.confirmForcePush": false,
    "editor.wordWrap": "on",
    "editor.matchBrackets": "near",
    "editor.mouseWheelZoom": true,
    "editor.renderControlCharacters": true,
    "editor.renderFinalNewline": false,
    "editor.renderLineHighlight": "all",
    "editor.smoothScrolling": true,
    "editor.tabCompletion": "on",
    "editor.cursorBlinking": "smooth",
    "editor.cursorSmoothCaretAnimation": true,
    "editor.find.autoFindInSelection": "multiline",
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.acceptSuggestionOnEnter": "smart",
    "editor.suggest.insertMode": "replace",
    "editor.suggest.insertHighlight": true,
    "files.insertFinalNewline": true,
    "files.simpleDialog.enable": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "workbench.commandPalette.history": 100,
    "workbench.commandPalette.preserveInput": true,
    "workbench.quickOpen.preserveInput": true,
    "workbench.editor.revealIfOpen": true,
    "window.title": "${dirty}${rootPath}${separator}${activeEditorShort}",
    "explorer.confirmDragAndDrop": false,
    "search.collapseResults": "auto",
    "search.maintainFileSearchCache": true,
    "search.quickOpen.includeSymbols": true,
    "debug.showBreakpointsInOverviewRuler": true,
    "terminal.integrated.cursorStyle": "underline",
    "terminal.integrated.cursorBlinking": true,
    "git.detectSubmodulesLimit": 20,
    "git.fetchOnPull": true,
    "merge-conflict.autoNavigateNextConflict.enabled": true,
    "python.pythonPath": "python3",
    "editor.cursorStyle": "line-thin"
